meme_dict = {
            "PANA": "Se refiere a amigo",
            "XD": "Una expresion de risa",
            "CRINGE": "Algo excepcionalmente raro o embarazoso",
            "LOL": "Una respuesta com√∫n a algo gracioso",
            }
print(meme_dict)
print("No entiendes una palabra buscula en el buscador con mayusculas")
word = input("Palabra:")

if word in meme_dict.keys():
    print(meme_dict[word])
    Y = input("Otra palabra que no entiendas Si/No:")
    
if Y=="Si" or Y=="si":
    word2 = input("Palabra:")
    
if word2 in meme_dict.keys():
    print(meme_dict[word2])
else:
    print("Palabra no encontrado")
